#### SCRIPT xxxx.sh ####
#!/bin/bash
seeds=(82 10 118 94)
selecs=(5 10 15 20 50)
clfs=(50 100 200)
ks=(41 44 42 43 45 46 47 48 49 50 60 70 80 90 100 200)
for k in ${ks[@]};
do
for clf in ${clfs[@]};
do
for selec in ${selecs[@]};
do
for see in ${seeds[@]};
do
sbatch --export=selec=$selec,clf=$clf,k=$k,cpt="",sele="",no_sele=0,filter=0,clini=0,seed=$see main.sh
done
done
done
done
########################

#### SCRIPT main.sh ####
#!/bin/bash
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=15
#SBATCH --mem=8G
#SBATCH --job-name=stratification
#SBATCH --time=4-00:00:00
